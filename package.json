{
  "name": "celeris-admin",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@7.11.0",
  "description": "Vite starter template with TailwindCSS",
  "keywords": [
    "celeris-admin",
    "vite",
    "tailwindcss",
    "fast",
    "boot"
  ],
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@kirklin/reset-css": "^0.0.2",
    "@vueuse/core": "^9.10.0",
    "element-plus": "^2.2.28",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.29",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@iconify-json/mdi": "^1.1.43",
    "@iconify-json/tabler": "^1.1.54",
    "@intlify/unplugin-vue-i18n": "^0.8.1",
    "@kirklin/eslint-config": "^0.2.2",
    "@types/node": "^18.11.18",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.27.1",
    "@vue/test-utils": "^2.2.7",
    "eslint": "^8.32.0",
    "eslint-plugin-tailwindcss": "^3.8.0",
    "husky": "^8.0.3",
    "jsdom": "^21.0.0",
    "lint-staged": "^13.1.0",
    "pnpm": "^7.25.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "unocss": "^0.48.4",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.4",
    "vitest": "^0.27.1",
    "vue-tsc": "^1.0.24"
  },
  "lint-staged": {
    "**/*": "eslint --fix"
  }
}
