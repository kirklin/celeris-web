    "@celeris/styles": "workspace:*",
    "@celeris/utils": "workspace:*",
    "@formily/core": "^2.2.24",
    "@formily/vue": "^2.2.24",
    "destr": "^1.2.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.3",
    "pinia-plugin-persistedstate": "^3.1.0",
    "pkg-types": "^1.0.3",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.1"
  },
  "devDependencies": {
    "@celeris/assets": "workspace:*",
