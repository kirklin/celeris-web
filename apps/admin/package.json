{
  "name": "@celeris/admin",
  "version": "0.0.1",
  "packageManager": "pnpm@8.5.1",
  "description": "Celeris Admin",
  "author": "Kirk Lin (https://github.com/kirklin)",
  "license": "MIT",
  "homepage": "https://github.com/kirklin/celeris-admin",
  "keywords": [
    "celeris-admin",
    "celeris",
    "vite",
    "unocss",
    "fast",
    "boot"
  ],
  "main": "src/main.ts",
  "scripts": {
